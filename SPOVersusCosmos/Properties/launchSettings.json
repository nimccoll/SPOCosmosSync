{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:25448",
      "sslPort": 44337
    }
  },
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "environmentVariables": {
        "TENANT": "{your SharePoint Tenant FQDN here}",
        "KEYVAULT_ENDPOINT": "{your Azure KeyVault URL here}",
        "SITE_URL": "{your SharePoint Site URL here}",
        "CLIENT_ID": "{your Client ID here}",
        "COSMOS_CONNECTIONSTRING": "{your CosmosDB connection string here}",
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "SPOVersusCosmos": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:5001;http://localhost:5000"
    }
  }
}